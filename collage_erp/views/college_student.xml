<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!--  window action  -->
    <record id="action_college_erp" model="ir.actions.act_window">
        <field name="name">College ERP</field>
        <field name="res_model">college.student</field>
        <field name="view_mode">list,form,kanban</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_similing_face">Create your first Student</p>
        </field>
    </record>

    <record id="college_student_view_tree" model="ir.ui.view">
        <field name="model">college.student</field>
        <field name="arch" type="xml">
             <list string="College" editable="bottom">
                <field name="name"/>
            </list>
        </field>
    </record>

</odoo>